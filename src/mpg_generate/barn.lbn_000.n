subroutine zero
*
linear 0 fpos bendto mlfoot   mlankle mlleg   mfmed1 mfmed2 mfmed3
linear 0 fpos bendto mlleg    mlknee  mlthigh 0 0 0
linear 0 fpos bendto mlthigh  mlhip   mpelvis 0 0 0
